<Window x:Class="StartCS_Delegate.Views.HistoryWindow.HistoryWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StartCS_Delegate.Views.HistoryWindow"
        mc:Ignorable="d"
        xmlns:vm="clr-namespace:StartCS_Delegate.ViewModels"
        Title="HistoryWindow" Height="450" Width="800">
    <Window.DataContext>
        <vm:MainWindowViewModel x:Name="ViewModel"/>
    </Window.DataContext>
    <Grid>
        <!--<ScrollViewer>
            <TextBlock x:Name="HistoryBlock" Text="Истории нет"/>
        </ScrollViewer>-->
        <!--<ListBox x:Name="HistoryKistBox"/>-->
        <ScrollViewer>
            <ItemsControl x:Name="HistoryControl"/>
        </ScrollViewer>
        <!--<ItemsControl x:Name="HistoryControl"/>-->
        <Button Content="Exit" Command="{Binding Path=BackToManagerWindowCommand}" HorizontalAlignment="Left" Margin="697,10,0,0" VerticalAlignment="Top" Width="93" Height="26"/>
        <Button Content="Clear" Command="{Binding Path=ClearHistoryCommand}" HorizontalAlignment="Left" Margin="697,41,0,0" VerticalAlignment="Top" Width="93" Height="26"/>
    </Grid>
</Window>
